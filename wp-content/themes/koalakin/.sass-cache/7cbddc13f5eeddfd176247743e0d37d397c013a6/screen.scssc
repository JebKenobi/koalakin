@import "modules/normalize";
@import "compass/css3/images";

// See modules/_base.scss
@import "modules/base";
@import "compass/css3/appearance";

@import "partials/header";
@import "partials/home";
@import "partials/footer";
@import "partials/ie";


body * {
	@include typeface;
    color: darken($brown, 10%);
}

h1, h2, h3, .serif {
	font-family: 'Kefa', Georgia, serif;
}
h1 {
	color: #bbeaed;
	font-size: 3em;
}
h2 {
	color: $green;
}
a {
	color: darken($green, 10%);
}
a.button {
	display: inline-block;
	width: auto;
	padding: 10px 30px;
	font-size: 1em;
	background-color: $teal;
	@include border-radius(5px);
	text-align: center;
	line-height: 1em;
	color: #fff;
	text-decoration: none;
}
a.button.rm {
	/*background: url(../images/arrow.png) 100% 50% $teal no-repeat;*/
	display: inline-block;
	padding: 10px 0 0px 10px;
	max-height: 25px;
	overflow: hidden;

	.icon {
		float: right;
		color: $teal;
		background-color: rgba(255,255,255,0.4);
		padding: 10px 7px;
		margin-top: -15px;
		margin-left: 15px;
		font-size: 1.7em;
	}
}
a.button:hover, a.button.rm:hover {
	background-color: $green;

	.icon {
		color: $green;
	}
}
.post-navigation {
	float: right;
	padding: 0 0 25px !important;
	width: 63.85%;
	float: right;
	margin: -3px 20% 0 0 !important;
	border-left: 3px dotted $lgray;
	display: block;
	max-width: 64%;

	.nav-links {
		border: none;
		display: inline-block;
		width: 90%;

		a {
			border: none;
			width: auto;
			display: inline-block;
			text-transform: uppercase;
		}

		a[rel="prev"] {
			float: left;
		}
		a[rel="next"] {
			float: right;
		}
	}
}
.cta2, .post-navigation .meta-nav, .comment-reply-link {
	text-transform: uppercase;
	text-decoration: none;
	padding: 0px 15px;
	@include border-radius(5px);
	font-size: 1em;
	font-weight: 100;
}
.cta2.green, .post-navigation .meta-nav:hover, .comment-reply-link {
	background-color: rgba(104,159,63,0.2);
	color: darken($green, 10%);
}
.cta2.teal, .post-navigation .meta-nav, .comment-reply-link:hover {
	background-color: rgba(107,202,208,0.2);
	color: $teal;
}
.comment-reply-link:before, .comment-reply-login:before {
	content: '';
}
.site, .site-header {
	max-width: 100%;
}
.site:before {
	display: none;
}
.site-content {
	margin-left: 0 !important;
}
.colLeft, .colRight {
	width: 45%;
	float: left;
	display: inline-block;
}
.colLeft {
	float: left;
}
.colRight {
	float: right;
}
.blogFeat, .featured-content .hentry {
	width: 30%;
	float:left;
	margin: 0 5% 20px 0;

	.thumb {
		background: $lgray;
		font-size: 0.7em;
		color: darken($lgray, 20%);
		display: inline-block;
		padding-bottom: 10px;

		img {
			width: 100%;
		}
		p {
			font-weight: lighter;
			color: darken($lgray, 20%);
			padding-top: 15px;
		}
		.cat {
			font-style: italic;
			float: left;
			margin-left: 7px;
			color: darken($lgray, 20%);

			a {
				color: darken($lgray, 20%);
			}
			a:hover {
				color: $teal;
			}
		}
		.date {
			text-transform: uppercase;
			float: right;
			margin-right: 7px;
			color: darken($lgray, 20%);
		}
	}
	p {
		font-size: 1.1em;
		font-weight: bold;
	}
}
.rightSide {
	float: right;

	.post {
		font-size: 1.1em;
		margin-bottom: 25px;
		a {
			color: $teal;
			font-weight: normal;
		}
		a:hover {
			color: $green;
		}
		p {
			margin-bottom: 0;
			padding-bottom: 0;
		}
		.cat a, .date {
			color: darken($lgray, 30%);
			font-style: italic;
			font-size: 0.8em;
			font-weight: lighter;
		}
		.cat a:hover {
			color: $green;
		}
	}

}
.sidebar {
	width: 25%;
	text-align: left;

	.title {
		font-weight: bold;
		text-transform: uppercase;
		clear: both;
		display: block;
		margin: 30px 0 7px;
		font-size: 1.2em;
	}
	ul {
		margin: 0;
		padding: 0;
		clear: both;
		display: inline-block;
		li {
			list-style-type: none;
			margin: 7px 0;

			a {
				text-decoration: none;
				font-size: 1.1em;
				@include border-radius(5px);
				font-weight: normal;
			}
		}
	}
	ul li.selected a {
		color: $dgray;
		background-color: rgba(104,159,63,0.1);
		font-weight: lighter;
	}
	ul#menu-social {
		display: inline-block;
		margin: 0;
		padding: 0;

		li {
			float: left;
			margin: 0 14px 0 0;
			padding: 0;
		}
		li.rss {
			margin-right: 0;
		}
	}
}
#featured-content {
	width: 100%;
	background: none;
	display: inline-block;
	padding: 0 0 20px;
	color: $dgray;

	a {
		color: darken($lgray, 30%);
	}
	.entry-title a {
		color: darken($green, 10%);
		text-transform: none;
	}
	.entry-title {
		text-align: left;
		font-weight: bold;
		font-size: 1.2em;
	}
	.entry-header {
		margin-top: 0;
		width: 100%;
		max-width: 100%;
		background-color: $lgray;
		text-align: left;

		.rm {
			color: #fff;
		}

		.entry-meta {
			background: none;
			color: $dgray;
			width: 100%;
			max-width: 100%;
			font-size: 0.8em;
			padding-bottom: 20px;

			.cat-links {
				float: left;
				text-transform: none;
				font-weight: normal;
				font-style: italic;

			}
			.date {
				float: right;
				font-weight: lighter;
			}
		}
	}
	.excerpt p {
		font-weight: lighter;
		font-size: 1em;
		text-align: left;
		margin: 15px 0;
	}

	.hentry {
		float: right;
		margin-right: 0;
	}

	.featured-content-inner article:first-of-type {
		width: 62%;
		float: left;
		background-color: $lgray;
		border-left: 15px solid $teal;
		font-size: 1.1em;
		padding-bottom: 20px;
		margin-right: 0;

		header {
			background: none;
		}

	}
}
.archive, .category, .single {
	#content {
		padding: 0 10%;
	}
	article {
		width: 77%;
		float: left;
		position: relative;
		padding: 0px;
		margin: 0;
	}
	.rightSide {
		width: 17%;
		float: right;
	}
}
.single {

	#content {
		padding-top: 150px;

		ul#menu-social {
			li a {
				background-position: 0 100%;
			}
			li.menu-item-76 a {
				background-position: $twi 100%;
			}
			li.menu-item-77 a {
				background-position: $pin 100%;
			}
			li.menu-item-78 a {
				background-position: $rss 100%;
			}
			li a:hover {
				background-position: 0 0%;
			}
			li.menu-item-76 a:hover {
				background-position: $twi 0%;
			}
			li.menu-item-77 a:hover {
				background-position: $pin 0%;
			}
			li.menu-item-78 a:hover {
				background-position: $rss 0%;
			}
			
		}
	
		article {
			text-align: left;
			position: relative;

			h1 {
				text-transform: none;
				color: $green;
				padding-bottom: 15px;
			}
			.entry-content {
				margin: 0;
				padding: 30px 0 !important;
				max-width: 100%;
				font-size: 1.1em;
			}
			.left-entry-meta {
				position: fixed;
				left: 40px;
				z-index: 50;

				.dot {
					left: 109.4% !important;
				}
			}
		}
		#comments {
			display: inline-block;
			width: 100%;
			max-width: 100%;
			padding: 0;
			margin-top: 0;
			background-color: #fff;
			z-index: 55;
			position: relative;

			
			.comment-list {
				width: 100%;
				max-width: 100%;

				article {
					width: 100%;
					border-top: none;
				}

				.reply {
					font-size: 1em;
					line-height: 2em;
					padding: 0;
					width: 59%;
					float: right;
					text-align: left;
					border-left: 3px dotted $lgray;
					padding: 25px 26% 0 2.5%;
					margin-top: 0;

					.comment-reply-link {
						padding: 3px 15px;
					}
				}
				.comment-content {
					width: 55.3%;
					float: right;
					font-size: 1.1em;
					border-left: 3px dotted $lgray;
					padding: 0px 26% 25px 2.5%;
					text-align: left;
				}
				.comment-meta {
				width: 13.5%;
				float: left;
				text-align: right;
				font-size: 1em;
				color: darken($lgray, 30%);
				padding-bottom: 25px;

				span {
					font-style: italic;
					color: darken($lgray, 30%);
				}
				a {
					font-weight: normal;
					text-transform: capitalize;
				}
				div.dot {
					background: darken($lgray, 30%);
					@include border-radius(7px);
					height:7px;
					width: 7px;
					position: absolute;
					top: 0px;
					left: 15.7%;
				}

				.comment-author, .comment-awaiting-moderation, .comment-content, .comment-list .reply, .comment-metadata {
					padding-left: 0;
				}
			}
			}
			#respond {
				max-width:64%;
				float: right;
				width: 61.2%;
				margin: -3px 20% 0 0 !important;
				border-left: 3px dotted $lgray;
				padding-left: 2.7%;
				padding-top: 25px;

				textarea:focus, input[type="text"]:focus {
					border: 2px solid #38a4f0;
				}
				textarea {
					width: 100%;
					height: 150px;
				}
				input[type="text"] {
					width: 30%;
					float: left;
					min-width: 150px;
					margin: 30px 5% 0 0;
				}
				input[type="text"]:last-of-type {
					margin-right: 0;
				}
				input[type="submit"] {
					width: 100%;
					clear: both;
					background-color: $green;
					font-weight: lighter;
					font-size: 1.1em;
					margin-top: 30px;
				}
				input[type="submit"]:hover, input[type="submit"]:focus {
					background-color: $teal;
				}
				input.hide {
					display: none;
				}

			}
		}
	}
}
#content article.post {
	.left-entry-meta {
		width: 18%;
		float: left;
